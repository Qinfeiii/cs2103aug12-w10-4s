<Window x:Class="Calendo.GoogleCalendar.AskAuth"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Authorization Code" Height="156" Width="300" WindowStyle="ToolWindow" ResizeMode="NoResize">
    <Grid Height="225">
        <TextBox Height="26" Margin="12,55,12,0" Name="textBox1" VerticalAlignment="Top" />
        <Button Content="Ok" Height="23" Margin="0,87,93,0" Name="button1" VerticalAlignment="Top" Click="button1_Click" HorizontalAlignment="Right" Width="75" />
        <TextBlock TextWrapping="Wrap" Text="In order to synchronize with Google Calendar, we would need your authorization code." Height="37" HorizontalAlignment="Left" Margin="15,12,0,0" Name="label1" VerticalAlignment="Top" Width="251" />
        <Button Content="Cancel" Margin="0,87,12,0" Name="button2" Height="23" VerticalAlignment="Top" HorizontalAlignment="Right" Width="75" Click="button2_Click" />
    </Grid>
</Window>
