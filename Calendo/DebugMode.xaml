<Window x:Class="Calendo.DebugMode"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Debug Mode" Height="300" Width="300" Loaded="Window_Loaded">
    <Grid>
        <TextBox Height="23" HorizontalAlignment="Left" Margin="12,12,0,0" Name="textBox1" VerticalAlignment="Top" Width="254" TextChanged="textBox1_TextChanged" KeyUp="textBox1_KeyUp" />
        <ListBox Height="208" HorizontalAlignment="Left" Margin="12,41,0,0" Name="listBox1" VerticalAlignment="Top" Width="178" />
        <Button Content="Add" Height="23" HorizontalAlignment="Left" Margin="196,43,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button1_Click" />
        <Button Content="Remove" Height="23" HorizontalAlignment="Left" Margin="196,72,0,0" Name="button2" VerticalAlignment="Top" Width="75" Click="button2_Click" />
        <Button Content="Undo" Height="23" HorizontalAlignment="Left" Margin="196,101,0,0" Name="button3" VerticalAlignment="Top" Width="75" Click="button3_Click" />
    </Grid>
</Window>
